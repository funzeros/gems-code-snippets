{
  "name": "gems-vue-snippets",
  "displayName": "Gems Vue Snippets",
  "description": "a custom snippets extension for vue by gems",
  "version": "0.0.4",
  "publisher": "GemsFang",
  "icon": "public/logo.png",
  "engines": {
    "vscode": "^1.62.3"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "vue",
        "path": "./snippets/snippets-vue.code-snippets"
      },
      {
        "language": "vue",
        "path": "./snippets/snippets-common.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/snippets-common.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/snippets-common.code-snippets"
      },
      {
        "language": "html",
        "path": "./snippets/snippets-common.code-snippets"
      }
    ]
  },
  "scripts": {
    "build": "vsce package"
  },
  "keywords": [
    "vue",
    "vue2",
    "vue3",
    "vuejs",
    "vue.js",
    "composition-api",
    "composition",
    "function-api",
    "function",
    "html",
    "javascript",
    "typescript",
    "snippet"
  ],
  "repository": {
    "type": "git",
    "url": "git@gitee.com:funzeros/gems-code-snippets.git"
  },
  "devDependencies": {
    "vsce": "2.6.3"
  }
}
